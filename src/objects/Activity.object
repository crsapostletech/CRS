    <fields>
        <fullName>Document_Link__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Subject = &apos;Send Credit Card Authorization (CCA)&apos;, HYPERLINK(&apos;/apex/CCAuthFormPreviewPage?reqId=&apos;&amp; WhatId &amp; &apos;&amp;taskId=&apos;&amp; Id, &apos;CC Auth Form&apos;), IF(Subject = &apos;Send Temporary Housing Agreement(THA)&apos;, HYPERLINK(&apos;/apex/THADocumentLanguagePage?whatId=&apos;&amp; WhatId &amp; &apos;&amp;taskId=&apos;&amp; Id , &apos;THA Document&apos;), IF(Subject = &apos;Send Adjuster Confirmation&apos;, HYPERLINK(&apos;/apex/AdjusterConfFormPreviewPage?reqId=&apos;&amp; WhatId &amp; &apos;&amp;taskId=&apos;&amp; Id, &apos;Adjuster Form&apos;), IF(CONTAINS(Subject, &apos;Send NTV&apos;), HYPERLINK(&apos;/apex/NTVDocumentPreviewPage?reqId=&apos;&amp; WhatId &amp; &apos;&amp;taskId=&apos;&amp; Id, &apos;NTV Document&apos;), &apos;&apos;) ) ) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Document Link</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
