    <validationRules>
        <fullName>Allstate_Message_Received</fullName>
        <active>true</active>
        <description>Verify there is a date/time in the Time Message Received field if the subject is Allstate Message Received.</description>
        <errorConditionFormula>AND(Subject = &quot;Allstate Message Received&quot;, ISBLANK( Time_Message_Received__c ))</errorConditionFormula>
        <errorDisplayField>Time_Message_Received__c</errorDisplayField>
        <errorMessage>Please enter the time the message was received.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Allstate_Message_Returned</fullName>
        <active>true</active>
        <description>Verify there is a date/time in the Time Message Returned field if the subject is Allstate Message Received.</description>
        <errorConditionFormula>AND(Subject = &quot;Allstate Message Received&quot;, ISBLANK( Time_Message_Received__c ))</errorConditionFormula>
        <errorDisplayField>Time_Message_Returned__c</errorDisplayField>
        <errorMessage>Please enter the time the message was returned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_edit_Closed_NTV_Tasks</fullName>
        <active>false</active>
        <description>Once an NTV Task is closed do not allow editing or deleting</description>
        <errorConditionFormula>AND(!ISCHANGED( RecordTypeId ) , RecordType.DeveloperName = &apos;NTV_Closed&apos;) &amp;&amp; NOT(OR(ISCHANGED(Fax_Completion_Time__c), ISCHANGED(Fax_Result_ID__c), ISCHANGED(Fax_Sent_User_ID__c), ISCHANGED(Fax_Status__c), ISCHANGED(Fax_Submit_Time__c)))</errorConditionFormula>
        <errorMessage>You cannot edit Cancelled or Completed NTV tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_edit_Completed_Portal_Note</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.DeveloperName = &apos;Portal_Notes_Completed&apos;</errorConditionFormula>
        <errorMessage>You cannot edit Completed Portal Notes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Portal_selected_on_Completed_Portal_Note</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;Portal_Notes&apos;, ISPICKVAL(Status, &apos;Completed&apos;), Publish_On_Adjuster_Portal__c = FALSE,  Publish_On_Policyholder_Portal__c = FALSE)</errorConditionFormula>
        <errorMessage>You must select a portal to push to before making the status Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Completed_Tasks_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Service_Request__c &lt;&gt; null, NOT(ISPICKVAL(Status, &quot;Completed&quot;)),ISPICKVAL(PRIORVALUE( Status), &quot;Completed&quot;))</errorConditionFormula>
        <errorMessage>Sorry, you can&apos;t change the status for completed tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Completed_Tasks_Type_Update</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Service_Request__c)),ISPICKVAL(PRIORVALUE(Status), &quot;Completed&quot;),ISCHANGED(Type) )</errorConditionFormula>
        <errorMessage>Restrict the users not to update the type when status is completed for service request tasks</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Task_Related_SR_InitialCallDateCheck</fullName>
        <active>true</active>
        <description>Checking the Task Related InitialCallDate Check</description>
        <errorConditionFormula>AND(ISBLANK( Service_Request__r.Initial_Call_Date__c ),
ISPICKVAL(Task_Follow_Up_Status__c,&quot;Attempt -1&quot;),
ISPICKVAL(Status,&quot;Completed&quot;))</errorConditionFormula>
        <errorMessage>Please Enter Initial Call Date In Service Request</errorMessage>
    </validationRules>
